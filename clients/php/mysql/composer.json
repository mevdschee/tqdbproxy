    "type": "library",
    "keywords": ["mysql", "pdo", "cache", "proxy", "tqdbproxy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maurits van der Schee",
            "email": "maurits@vdschee.nl"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TQDBProxy\\MySQL\\": "src/"
