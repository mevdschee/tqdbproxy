{
    "name": "mevdschee/tqdbproxy-postgres",
    "description": "PostgreSQL client for TQDBProxy with TTL-aware caching",
    "type": "library",
    "keywords": [
        "postgresql",
        "postgres",
        "pdo",
        "cache",
        "proxy",
        "tqdbproxy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maurits van der Schee",
            "email": "maurits@vdschee.nl"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TQDBProxy\\Postgres\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQDBProxy\\Postgres\\Tests\\": "tests/"
        }
    }
}