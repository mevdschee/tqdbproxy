{
    "name": "mevdschee/tqdbproxy-mariadb",
    "description": "MariaDB client for TQDBProxy with TTL-aware caching",
    "type": "library",
    "keywords": [
        "mariadb",
        "pdo",
        "cache",
        "proxy",
        "tqdbproxy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maurits van der Schee",
            "email": "maurits@vdschee.nl"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TQDBProxy\\MariaDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQDBProxy\\MariaDB\\Tests\\": "tests/"
        }
    }
}